{
    "name": "keepers-team/webtlo",
    "description": "Simple application for torrent management",
    "type": "project",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "license": "GPLv3",
    "autoload": {
        "psr-4": {
            "KeepersTeam\\Webtlo\\": "back/"
        }
    },
    "require": {
        "php": "^8.1",
        "phpxmlrpc/polyfill-xmlrpc": "^1.0@RC",
        "league/container": "^4.2",
        "psr/log": "^3.0",
        "monolog/monolog": "^3.5",
        "guzzlehttp/guzzle": "^7.8",
        "caseyamcl/guzzle_retry_middleware": "^2.9",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-pdo": "*",
        "ext-xml": "*",
        "ext-sqlite3": "*"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^1.10"
    }
}
