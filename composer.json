{
    "name": "keepers-team/webtlo",
    "description": "Simple application for torrent management",
    "type": "project",
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.13",
        "phpstan/phpstan": "^1.9"
    },
    "license": "GPLv3",
    "autoload": {
        "psr-4": {
            "KeepersTeam\\Webtlo\\": "src/"
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/Wedrix/comet"
        }
    ],
    "require": {
        "ext-pdo": "*",
        "ext-curl": "*",
        "ext-mbstring": "*",
        "ext-sqlite3": "*",
        "ext-dom": "*",
        "ext-xml": "*",
        "ext-libxml": "*",
        "ext-simplexml": "*",
        "phpxmlrpc/polyfill-xmlrpc": "1.0.0-rc1",
        "gotzmann/comet": "dev-master#7bb200f664e59caf2c99eeaa4f5da2c38d212ebf",
        "monolog/monolog": "^3.2",
        "splitbrain/php-cli": "^1.2",
        "guzzlehttp/guzzle": "^7.0",
        "caseyamcl/guzzle_retry_middleware": "^2.8",
        "uma/dic": "^3.0"
    }
}
